  title = {Left Ventricular Diastolic and Systolic Dyssynchrony and Dysfunction in Heart Failure with Preserved Ejection Fraction and a Narrow {{QRS}} Complex},
  author = {Liu, Shuang and Guan, Zhengyu and Jin, Xuanyi and Meng, Pingping and Wang, Yonghuai and Zheng, Xianfeng and Jia, Dalin and Ma, Chunyan and Yang, Jun},
