@article{linde_interactionsexheight_2018,
  title = {The Interaction of Sex, Height, and {{QRS}} Duration on the Effects of Cardiac Resynchronization Therapy on Morbidity and Mortality: An Individual-Patient Data Meta-Analysis},
  shorttitle = {The Interaction of Sex, Height, and {{QRS}} Duration on the Effects of Cardiac Resynchronization Therapy on Morbidity and Mortality},
  author = {Linde, Cecilia and Cleland, John G. F. and Gold, Michael R. and Daubert, J. Claude and Tang, Anthony S. L. and Young, James B. and Sherfesee, Lou and Abraham, William T.},
  year = {2018},
  journal = {Eur. J. Heart Fail.},
  volume = {20},
  number = {4},
  pages = {780--791},
  issn = {1879-0844},
  doi = {10/gdrfdj},
  abstract = {Aims To explore possible associations that may explain the greater benefit from cardiac resynchronization therapy (CRT) reported amongst women. Methods and results In an individual-patient data meta-analysis of five randomized controlled trials, all-cause mortality and the composite of all-cause mortality or first hospitalization for heart failure (HF) were compared among 794 women and 2702 men assigned to CRT or a control group. Multivariable analyses were performed to assess the impact of sex, QRS duration, HF aetiology, left ventricular end-diastolic diameter (LVEDD), and height on outcome. Women were shorter, had smaller LVEDD, more often left bundle branch block, and less often ischaemic heart disease, but QRS duration was similar between sexes. Women tended to obtain greater benefit from CRT but sex was not an independent predictor of either outcome. For all-cause mortality, QRS duration was the only independent predictor of CRT benefit. For the composite outcome, height and QRS duration, but not sex, were independent predictors of CRT benefit. Further analysis suggested increasing benefit with increasing QRS duration amongst shorter patients, of whom a great proportion were women. Conclusions In this individual-patient data meta-analysis, CRT benefit was greater in shorter patients, which may explain reports of enhanced CRT benefit among women. Further analyses are required to determine whether recommendations on the QRS threshold for CRT should be adjusted for height. (ClinicalTrials.gov numbers: NCT00170300, NCT00271154, NCT00251251).},
  copyright = {\textcopyright{} 2018 The Authors. European Journal of Heart Failure \textcopyright{} 2018 European Society of Cardiology},
  langid = {english},
  keywords = {Cardiac resynchronization therapy,Gender,Heart failure,Height},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\EQ2G9YAY\\Linde et al_2018_The interaction of sex, height, and QRS duration on the effects of cardiac.pdf;C\:\\Users\\loewe\\Documents\\Zotero\\storage\\TB7GXSDF\\ejhf.html}
