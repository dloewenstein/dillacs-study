@article{tung_definingleftbundle_2020,
  title = {Defining {{Left Bundle Branch Block Patterns}} in {{Cardiac Resynchronisation Therapy}}: {{A Return}} to {{His Bundle Recordings}}},
  shorttitle = {Defining {{Left Bundle Branch Block Patterns}} in {{Cardiac Resynchronisation Therapy}}},
  author = {Tung, Roderick and Upadhyay, Gaurav A},
  year = {2020},
