}

@article{verbeek_quantificationinterventricularasynchrony_2002,
  title = {Quantification of Interventricular Asynchrony during {{LBBB}}  and Ventricular Pacing},
  author = {Verbeek, Xander A. A. M. and Vernooy, Kevin and Peschar, Maaike and {Van der Nagel}, Theo and Van Hunnik, Arne and Prinzen, Frits W.},
  year = {2002},
  month = oct,
  journal = {Am. J. Physiol.-Heart Circ. Physiol.},
  volume = {283},
  number = {4},
  pages = {H1370-H1378},
  publisher = {{American Physiological Society}},
  issn = {0363-6135},
  doi = {10.1152/ajpheart.00051.2002},
  abstract = {The quantification of mechanical interventricular asynchrony (IVA) was investigated. In 12 dogs left bundle branch block (LBBB) was induced by radio frequency ablation. Left ventricular (LV) and right ventricular (RV) pressures were recorded before and after induction of LBBB and during LBBB + LV apex pacing at different atrioventricular (AV) delays. Four IVA measures were validated using computer simulations on experimentally obtained pressure signals. The most robust measure for IVA was the time delay between the upslope of the LV and RV pressure signals ({$\Delta$}T up), estimated by cross correlation. The induction of experimental LBBB decreased {$\Delta$}T upfrom -6.9 {$\pm$} 7.0 ms (RV before LV) to -33.9 {$\pm$} 7.6 ms (P {$<$} 0.05) in combination with a significant decrease of LV maximal first derivative of pressure development over time (dP/dt max). During LV apex pacing, {$\Delta$}T up increased with decreasing AV delay up to +20.9 {$\pm$} 14.6 ms (P {$<$} 0.05). Interventricular resynchronization ({$\Delta$}T up = 0 ms) significantly improved LV dP/dt max by 15.1 {$\pm$} 5.9\%. QRS duration increased significantly after induction of LBBB but did not change during LV apex pacing. In conclusion, {$\Delta$}T up is a reliable measure of mechanical IVA, which adds valuable information concerning the nature of asynchronous activation of the ventricles.},
  keywords = {left bundle branch block,resynchronization},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\YXYB2R4V\\Verbeek et al_2002_Quantification of interventricular asynchrony during LBBB and ventricular.pdf}
