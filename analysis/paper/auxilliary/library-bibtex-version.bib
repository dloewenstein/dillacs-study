@article{davison_acuteheartfailure_2021,
  title = {Is Acute Heart Failure a Distinctive Disorder? {{An}} Analysis from {{BIOSTAT-CHF}}},
  shorttitle = {Is Acute Heart Failure a Distinctive Disorder?},
  author = {Davison, Beth A. and Senger, Stefanie and Sama, Iziah E. and Koch, Gary G. and Mebazaa, Alexandre and Dickstein, Kenneth and Samani, Nilesh J. and Metra, Marco and Anker, Stefan D. and Cleland, John G. and Ng, Leong L. and Mordi, Ify R. and Zannad, Faiez and Filippatos, Gerasimos S. and Hillege, Hans L. and Ponikowski, Piotr and van Veldhuisen, Dirk J. and Lang, Chim C. and van der Meer, Peter and N{\'u}{\~n}ez, Julio and {Bay{\'e}s-Gen{\'i}s}, Antoni and Edwards, Christopher and Voors, Adriaan A. and Cotter, Gad},
  year = {2021},
  journal = {Eur. J. Heart Fail.},
  volume = {23},
  number = {1},
  pages = {43--57},
  issn = {1879-0844},
  doi = {10/gkgdfg},
  abstract = {Aims This retrospective analysis sought to identify markers that might distinguish between acute heart failure (HF) and worsening HF in chronic outpatients. Methods and results The BIOSTAT-CHF index cohort included 2516 patients with new or worsening HF symptoms: 1694 enrolled as inpatients (acute HF) and 822 as outpatients (worsening HF in chronic outpatients). A validation cohort included 935 inpatients and 803 outpatients. Multivariable models were developed in the index cohort using clinical characteristics, routine laboratory values, and proteomics data to examine which factors predict adverse outcomes in both conditions and to determine which factors differ between acute HF and worsening HF in chronic outpatients, validated in the validation cohort. Patients with acute HF had substantially higher morbidity and mortality (6-month mortality was 12.3\% for acute HF and 4.7\% for worsening HF in chronic outpatients). Multivariable models predicting 180-day mortality and 180-day HF readmission differed substantially between acute HF and worsening HF in chronic outpatients. Carbohydrate antigen 125 was the strongest single biomarker to distinguish acute HF from worsening HF in chronic outpatients, but only yielded a C-index of 0.71. A model including multiple biomarkers and clinical variables achieved a high degree of discrimination with a C-index of 0.913 in the index cohort and 0.901 in the validation cohort. Conclusions This study identifies different characteristics and predictors of outcome in acute HF patients as compared to outpatients with chronic HF developing worsening HF. The markers identified may be useful in better diagnosing acute HF and may become targets for treatment development.},
  langid = {english},
  keywords = {Acute heart failure,Diagnosis,Treatment},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ejhf.2077},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\HMZQ2XYS\\Davison et al_2021_Is acute heart failure a distinctive disorder.pdf;C\:\\Users\\loewe\\Documents\\Zotero\\storage\\KZWDX9TL\\ejhf.html}
}

@article{deegan_naturalhistoryfabry_2006,
  title = {Natural History of {{Fabry}} Disease in Females in the {{Fabry Outcome Survey}}},
  author = {Deegan, P. B. and Baehner, A. F. and Romero, M.-{\'A} Barba and Hughes, D. A. and Kampmann, C. and Beck, M.},
  year = {2006},
  month = apr,
  journal = {J. Med. Genet.},
  volume = {43},
  number = {4},
  pages = {347--352},
  issn = {0022-2593, 1468-6244},
  doi = {10/cgfmhr},
  abstract = {Background: Fabry disease is a rare X linked lysosomal storage disorder resulting from deficiency of {$\alpha$}-galactosidase A activity. Although the severity of clinical features in male patients is well described, only recently have studies reported the high prevalence of disabling clinical features in heterozygous females. Aims: This study sets out to examine the clinical features and natural history of Fabry disease in further detail in a large group of female patients. Methods: Data were obtained from 303 females enrolled in the Fabry Outcome Survey. Pain was assessed using the Brief Pain Inventory, and health related quality of life (HRQoL) was assessed using the European Quality of Life Questionnaire. A modified version of the Mainz Severity Score Index was also applied. Data on left ventricular mass (LVM) index, mean ventricular wall thickness, and glomerular filtration rate (GFR) were used to assess cardiac and renal involvement. Results: The most commonly reported clinical features in females were neurological (77\%) and cardiac (59\%). A history of renal involvement was recorded in 40\% of cases. Neurological features were the earliest to develop (mean age: 16 years), whereas cardiac (mean age: 33.5 years) and renal (mean age: 37.3 years) features developed later. LVM index increased exponentially with age. In addition, age was negatively correlated with estimated GFR and HRQoL. Conclusions: Females with Fabry disease report important age related clinical features and clinical investigation demonstrates evidence of disease progression. This study highlights the importance of careful and longitudinal assessment of female heterozygote patients with Fabry disease.},
  copyright = {Copyright 2006 Journal of Medical Genetics},
  langid = {english},
  pmid = {16227523},
  keywords = {BPI; Brief Pain Inventory,EQ-5D; European Quality of Life Questionnaire,ERT; enzyme replacement therapy,Fabry disease,Fabry Outcome Survey,FOS; the Fabry Outcome Survey,GFR; glomerular filtration rate,heterozygotes,HRQoL; health related quality of life,LVM; left ventricular mass,MDRD; Modification of Diet in Renal Disease,MSSI; Mainz Severity Score Index},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\CF5N2FLI\\Deegan et al_2006_Natural history of Fabry disease in females in the Fabry Outcome Survey.pdf;C\:\\Users\\loewe\\Documents\\Zotero\\storage\\6HMG64KZ\\347.html}
}

