@article{orszulak_regionalstrainpattern_2021,
  title = {Regional {{Strain Pattern Index}}\textemdash{{A Novel Technique}} to {{Predict CRT Response}}},
  author = {Orszulak, Micha{\l} and Filipecki, Artur and Wr{\'o}bel, Wojciech and {Berger-Kucza}, Adrianna and Orszulak, Witold and {Urba{\'n}czyk-Swi{\'c}}, Dagmara and Kwa{\'s}niewski, Wojciech and {P{\l}o{\'n}ska-Go{\'s}ciniak}, Edyta and {Mizia-Stec}, Katarzyna},
  year = {2021},
  month = jan,
  journal = {IJERPH},
  volume = {18},
  number = {3},
  pages = {926},
  issn = {1660-4601},
  doi = {10/gjvd97},
  abstract = {Background: Cardiac resynchronization therapy (CRT) improves outcome in patients with heart failure (HF) however approximately 30\% of patients still remain non-responsive. We propose a novel index\textemdash Regional Strain Pattern Index (RSPI)\textemdash to prospectively evaluate response to CRT. Methods: Echocardiography was performed in 49 patients with HF (66.5 {$\pm$} 10 years, LVEF 24.9 {$\pm$} 6.4\%, QRS width 173.1 {$\pm$} 19.1 ms) two times: before CRT implantation and 15 {$\pm$} 7 months after. At baseline, dyssynchrony was assessed including RSPI and strain pattern. RSPI was calculated from all three apical views across 12 segments as the sum of dyssynchronous components. From every apical view, presence of four components were assessed: (1) contraction of the early-activated wall; (2) prestretching of the late activated wall; (3) contraction of the early-activated wall in the first 70\% of the systolic ejection phase; (4) peak contraction of the late-activated wall after aortic valve closure. Each component scored 1 point, thus the maximum was 12 points. Results: Responders reached higher mean RSPI values than non-responders (5.86 {$\pm$} 2.9 vs. 4.08 {$\pm$} 2.4; p = 0.044). In logistic regression analysis value of RSPI {$\geq$} 7 points was a predictor of favorable CRT effect (OR: 12; 95\% CI = 1.33\textendash 108.17; p = 0.004). Conclusions: RSPI could be a valuable predictor of positive outcome in HF patients treated with CRT.},
  langid = {english},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\QTFHTM9Z\\Orszulak et al. - 2021 - Regional Strain Pattern Index—A Novel Technique to.pdf}
}

