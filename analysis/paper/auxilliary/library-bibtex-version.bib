@article{sun_inappropriateusebivariable_1996,
  title = {Inappropriate Use of Bivariable Analysis to Screen Risk Factors for Use in Multivariable Analysis},
  author = {Sun, Guo-Wen and Shook, Thomas L. and Kay, Gregory L.},
  year = {1996},
  month = aug,
  journal = {Journal of Clinical Epidemiology},
  volume = {49},
  number = {8},
  pages = {907--916},
  issn = {0895-4356},
  doi = {10/ck9mbc},
  abstract = {The use of bivariable selection (BVS) for selecting variables to be used in multivariable analysis is inappropriate despite its common usage in medical sciences. In BVS, if the statistical p value of a risk factor in bivariable analysis is greater than an arbitrary value (often p = 0.05), then this factor will not be allowed to compete for inclusion in multivariable analysis. This type of variable selection is inappropriate because the BVS method wrongly rejects potentially important variables when the relationship between an outcome and a risk factor is confounded by any confounder and when this confounder is not properly controlled. This article uses both hypothetical and actual data to show how a nonsignificant risk factor in bivariable analysis may actually be a significant risk factor in multivariable analysis if confounding is properly controlled. Furthermore, problems resulting from the automated forward and stepwise modeling with or without the presence of confounding are also addressed. To avoid these improper procedures and deficiencies, alternatives in performing multivariable analysis, including advantages and disadvantages of the BVS method and automated stepwise modeling, are reviewed and discussed.},
  keywords = {bias,bivariable analysis,confounding,multivariable analysis,Risk factors,variable selection},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\SZUIYTHN\\Sun et al_1996_Inappropriate use of bivariable analysis to screen risk factors for use in.pdf;C\:\\Users\\loewe\\Documents\\Zotero\\storage\\JSK8FGE6\\089543569600025X.html}
}

@article{suppogu_anginarelatescoronary_2021,
  title = {Angina Relates to Coronary Flow in Women with Ischemia and No Obstructive Coronary Artery Disease},
  author = {Suppogu, Nissi and Wei, Janet and Quesada, Odayme and Shufelt, Chrisandra and {Cook-Wiens}, Galen and Samuels, Bruce and Petersen, John W. and Anderson, R. David and Handberg, Eileen M. and Pepine, Carl J. and Merz, C. Noel Bairey},
  year = {2021},
  month = jun,
  journal = {International Journal of Cardiology},
  volume = {333},
  pages = {35--39},
  publisher = {{Elsevier}},
  issn = {0167-5273, 1874-1754},
  doi = {10/gjwz5c},
  abstract = {{$<$}h2{$>$}Abstract{$<$}/h2{$><$}h3{$>$}Background{$<$}/h3{$><$}p{$>$}Women with suspected ischemia and no obstructive coronary artery disease (INOCA) often have coronary microvascular dysfunction (CMD) as measured by impaired coronary flow reserve (CFR), which is associated with angina and adverse cardiovascular events. CFR is a ratio of hyperemic to baseline average peak velocity (bAPV), and the relation of baseline flow to angina is not understood.{$<$}/p{$><$}h3{$>$}Methods{$<$}/h3{$><$}p{$>$}We evaluated 259 women enrolled in the WISE-Coronary Vascular Dysfunction (WISE-CVD) project with suspected CMD who underwent invasive coronary functional testing. We analyzed variables stratified by high (e.g. {$\geq$}22 cm/s) vs low ({$<$}22 cm/s) bAPV, using \emph{t-}test or Wilcoxon rank; linear and multivariable regression was used with bAPV as a continuous variable.{$<$}/p{$><$}h3{$>$}Results{$<$}/h3{$><$}p{$>$}Women with high bAPV had worse Seattle Angina Questionnaire (SAQ) angina frequency (58 {$\pm$} 26 vs 67 {$\pm$} 25, \emph{p} = 0.005) and SAQ-7 scores (57 {$\pm$} 22 vs 62 {$\pm$} 21, \emph{p} = 0.03), with higher nitrate (\emph{p} = 0.02) and ranolazine use (p = 0.03). The high bAPV subgroup also had lower CFR (\emph{p} {$<$} 0.001)). Linear regression related higher bAPV with lower SAQ-7 (\emph{p} = 0.01) and lower angina frequency scores (\emph{p} = 0.001). These results remained significant in multivariable modelling adjusting for baseline differences (\emph{p} {$<$} 0.04). SAQ-7 was significantly predicted by bAPV.{$<$}/p{$><$}h3{$>$}Conclusions{$<$}/h3{$><$}p{$>$}Among women with suspected INOCA, angina relates to high bAPV, a result supported by the concomitant greater use of anti-anginal drugs. These results suggest that high bAPV contributes to impaired CFR and may represent a specific pathophysiologic contributor to CMD and may be a treatment target in INOCA subjects.{$<$}/p{$>$}},
  langid = {english},
  pmid = {33662486},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\S65BGJFF\\Suppogu et al_2021_Angina relates to coronary flow in women with ischemia and no obstructive.pdf;C\:\\Users\\loewe\\Documents\\Zotero\\storage\\XPDNZMCC\\fulltext.html}
