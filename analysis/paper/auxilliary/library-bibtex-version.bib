@article{kirn_mechanicaldiscoordinationrather_2008,
  title = {Mechanical Discoordination Rather than Dyssynchrony Predicts Reverse Remodeling upon Cardiac Resynchronization},
  author = {Kirn, Borut and Jansen, Annemieke and Bracke, Frank and {van Gelder}, Berry and Arts, Theo and Prinzen, Frits W.},
  year = {2008},
  month = aug,
  journal = {American Journal of Physiology-Heart and Circulatory Physiology},
  volume = {295},
  number = {2},
  pages = {H640-H646},
  issn = {0363-6135},
  doi = {10/dd32vb},
  abstract = {By current guidelines a considerable part of the patients selected for cardiac resynchronization therapy (CRT) do not respond to the therapy. We hypothesized that mechanical discoordination [opposite strain within the left ventricular (LV) wall] predicts reversal of LV remodeling upon CRT better than mechanical dyssynchrony. MRI tagging images were acquired in CRT candidates (n = 19) and in healthy control subjects (n = 9). Circumferential strain ({$\epsilon$}cc) was determined in 160 regions. From {$\epsilon$}cc signals we derived 1) an index of mechanical discoordination [internal stretch fraction (ISF), defined as the ratio of stretch to shortening during ejection] and 2) indexes of mechanical dyssynchrony: the 10\textendash 90\% width of time to onset of shortening, time to peak shortening, and end-systolic strain. LV end-diastolic volume (LVEDV), end-systolic volume (LVESV), and ejection fraction (LVEF) were determined before and after 3 mo of CRT. Responders were defined as those patients in whom LVESV decreased by {$>$}15\%. In responders (n = 10), CRT increased LVEF and decreased LVEDV and LVESV (11 {$\pm$} 6\%, 21 {$\pm$} 16\%, and 30 {$\pm$} 16\%, respectively) significantly more (P {$<$} 0.05) than in nonresponders (1 {$\pm$} 6\%, 3 {$\pm$} 4\%, and 5 {$\pm$} 10\%, respectively). Among mechanical indexes, only ISF was different between responders and nonresponders (0.53 {$\pm$} 0.25 vs. 0.31 {$\pm$} 0.16; P {$<$} 0.05). In patients with ISF {$>$}0.4 (n = 10), LVESV decreased by 31 {$\pm$} 18\% vs. 5 {$\pm$} 11\% in patients with ISF {$<$}0.4 (P {$<$} 0.05). We conclude that mechanical discoordination, as estimated from ISF, is a better predictor of reverse remodeling after CRT than differences in time to onset and time to peak shortening. Therefore, discoordination rather than dyssynchrony appears to reflect the reserve contractile capacity that can be recruited by CRT.},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\V52Q2A6T\\Kirn et al_2008_Mechanical discoordination rather than dyssynchrony predicts reverse remodeling.pdf;C\:\\Users\\loewe\\Documents\\Zotero\\storage\\VIM8K33B\\ajpheart.00106.html}
}

@article{kirsch_comparisontwoportable_2019,
