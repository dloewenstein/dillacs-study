@article{sohal_combinedidentificationseptal_2014,
  title = {Combined Identification of Septal Flash and Absence of Myocardial Scar by Cardiac Magnetic Resonance Imaging Improves Prediction of Response to Cardiac Resynchronization Therapy},
  author = {Sohal, Manav and Amraoui, Sana and Chen, Zhong and Sammut, Eva and Jackson, Tom and Wright, Matthew and O'Neill, Mark and Gill, Jaswinder and {Carr-White}, Gerald and Rinaldi, C. Aldo and Razavi, Reza},
  year = {2014},
