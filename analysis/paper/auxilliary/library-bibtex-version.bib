@article{reiter_bloodflowvortices_2014,
  title = {Blood {{Flow Vortices}} along the {{Main Pulmonary Artery Measured}} with {{MR Imaging}} for {{Diagnosis}} of {{Pulmonary Hypertension}}},
  author = {Reiter, Gert and Reiter, Ursula and Kovacs, Gabor and Olschewski, Horst and Fuchsj{\"a}ger, Michael},
