@article{depooter_relationelectricalmechanical_2018,
  title = {Relation between Electrical and Mechanical Dyssynchrony in Patients with Left Bundle Branch Block: {{An}} Electro- and Vectorcardiographic Study},
  shorttitle = {Relation between Electrical and Mechanical Dyssynchrony in Patients with Left Bundle Branch Block},
  author = {De Pooter, Jan and El Haddad, Milad and Kamoen, Victor and Kallupurackal, Thomas Tibin and Stroobandt, Roland and De Buyzere, Marc and Timmermans, Frank},
  year = {2018},
  month = jul,
  journal = {Ann. Noninvasive Electrocardiol.},
  volume = {23},
  number = {4},
  pages = {e12525},
  issn = {1082-720X},
  doi = {10/gcpmdk},
  abstract = {BackgroundCurrent guidelines select patients for cardiac resynchronization therapy (CRT) mainly on electrocardiographic parameters like QRS duration and left bundle branch block (LBBB). However, among those LBBB patients, heterogeneity in mechanical dyssynchrony occurs and might be a reason for nonresponse to CRT. This study assesses the relation between electrocardiographic characteristics and presence of mechanical dyssynchrony among LBBB patients. MethodsThe study included patients with true LBBB (including mid-QRS notching) on standard 12-lead electrocardiograms. Left bundle branch block-induced mechanical dyssynchrony was assessed by the presence of septal flash on two-dimensional echocardiography. Previously reported electro- and vectorcardiographic dyssynchrony markers were analyzed: global QRS duration (QRSD(LBBB)), left ventricular activation time (QRSD(LVAT)), time to intrinsicoid deflection (QRSD(ID)), and vectorcardiographic QRS areas in the 3D vector loop (QRSA(3D)). ResultsThe study enrolled 545 LBBB patients. Septal flash (SF) is present in 52\% of patients presenting with true LBBB. Patients with SF are more frequent female, have less ischemic heart disease and smaller left ventricular dimensions. In multivariate analysis longer QRSD(LBBB), QRSD(LVAT) and larger QRSA(3D) were independently associated with SF. Of all parameters, QRSA(3D) has the best accuracy to predict SF, although overall accuracy remains moderate (59\% sensitivity, 58\% specificity). The predictive value of QRSA(3D) remained constant in both sexes, irrespective of ischemic heart disease, ejection fraction and even when categorizing for QRSD(LBBB). ConclusionIn LBBB patients, large QRS areas correlate better with mechanical dyssynchrony compared to wide QRSD intervals. However, the overall accuracy to predict mechanical dyssynchrony by electrocardiographic dyssynchrony markers, even when using complex vectorcardiographic parameters, remains low.},
  langid = {english},
  keywords = {association,candidates,cardiac resynchronization therapy,cardiology,dyssynchrony,electrocardiography,heart-failure,qrs duration,recommendations,septal   flash,vectorcardiography and left bundle branch block,ventricular activation},
  annotation = {WOS:000437669400006},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\7IVBWBQ4\\De Pooter et al_2018_Relation between electrical and mechanical dyssynchrony in patients with left.pdf}
}

