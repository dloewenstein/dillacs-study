@article{janssen_missingcovariatedata_2010,
  title = {Missing Covariate Data in Medical Research: {{To}} Impute Is Better than to~Ignore},
  shorttitle = {Missing Covariate Data in Medical Research},
  author = {Janssen, Kristel J. M. and Donders, A. Rogier T. and Harrell, Frank E. and Vergouwe, Yvonne and Chen, Qingxia and Grobbee, Diederick E. and Moons, Karel G. M.},
  year = {2010},
