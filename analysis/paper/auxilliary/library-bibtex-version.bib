  shorttitle = {Circumferential Myocardial Strain in Cardiomyopathy with and without Left Bundle Branch Block},
  author = {Han, Y. C. and Chan, J. and Haber, I. and Peters, D. C. and Zimetbaum, P. J. and Manning, W. J. and Yeon, S. B.},
  year = {2010},
