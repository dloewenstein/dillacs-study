@article{quarta_cardiovascularmagneticresonance_2020,
  ids = {quarta_cardiovascularmagneticresonance_},
  title = {Cardiovascular Magnetic Resonance in Heart Failure with Preserved Ejection Fraction: Myocyte, Interstitium, Microvascular, and Metabolic Abnormalities},
  shorttitle = {Cardiovascular Magnetic Resonance in Heart Failure with Preserved Ejection Fraction},
  author = {Quarta, Giovanni and Gori, Mauro and Iorio, Annamaria and D'Elia, Emilia and Moon, James C. and Iacovoni, Attilio and Burocchi, Simone and Schelbert, Erik B. and Brambilla, Paolo and Sironi, Sandro and Caravita, Sergio and Parati, Gianfranco and Gavazzi, Antonello and Maisel, Alan S. and Butler, Javed and Lam, Carolyn SP and Senni, Michele},
  year = {2020},
  month = jul,
  journal = {Eur. J. Heart Fail.},
  volume = {n/a},
  number = {n/a},
  issn = {1879-0844},
  doi = {10/gg573k},
  abstract = {Heart failure (HF) with preserved ejection fraction (HFpEF) is a chronic cardiac condition whose prevalence continues to rise, with high social and economic burden, but with no specific approved treatment. Patients diagnosed with HFpEF have a high prevalence of comorbidities and there is a likely high misdiagnosis rate. True HFpEF is likely to have multiple pathophysiological causes \textendash{} with these causes themselves clinically ill-defined through limitations of current measurement techniques. Myocyte, interstitium, microvascular, and metabolic abnormalities have been regarded as key components of the pathophysiology and potential therapeutic targets. Cardiac magnetic resonance (CMR) has the capability to look deeper with a number of tissue characterization techniques which are closer to the underlying specific abnormalities and which could be linked to personalized medicine for HFpEF. This review aims to discuss the potential role of CMR to better define HFpEF phenotypes and to infer measurable therapeutic targets.},
  copyright = {This article is protected by copyright. All rights reserved.},
  langid = {english},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ejhf.1961},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\4MRY6A8X\\Quarta et al_Cardiovascular magnetic resonance in heart failure with preserved ejection.pdf;C\:\\Users\\loewe\\Documents\\Zotero\\storage\\QA8B8862\\Quarta et al_Cardiovascular magnetic resonance in heart failure with preserved ejection.pdf;C\:\\Users\\loewe\\Documents\\Zotero\\storage\\BRKNJM3Q\\ejhf.html;C\:\\Users\\loewe\\Documents\\Zotero\\storage\\ZAEK8M39\\ejhf.html}
}

@article{rabbat_futurecardiacmagnetic_2021,
  title = {The {{Future}} of {{Cardiac Magnetic Resonance Clinical Trials}}},
  author = {Rabbat, Mark G. and Kwong, Raymond Y. and Heitner, John F. and Young, Alistair A. and Shanbhag, Sujata M. and Petersen, Steffen E. and Selvanayagam, Joseph B. and Berry, Colin and Nagel, Eike and Heydari, Bobak and Maceira, Alicia M. and Shenoy, Chetan and Dyke, Christopher and Bilchick, Kenneth C.},
  year = {2021},
  month = dec,
  journal = {JACC: Cardiovascular Imaging},
  issn = {1936-878X},
  doi = {10.1016/j.jcmg.2021.07.029},
  abstract = {Over the past 2 decades, cardiac magnetic resonance (CMR) has become an essential component of cardiovascular clinical care and contributed to imaging-guided diagnosis and management of coronary artery disease, cardiomyopathy, congenital heart disease, cardio-oncology, valvular, and vascular disease, amongst others. The widespread availability, safety, and capability of CMR to provide corresponding anatomical, physiological, and functional data in 1 imaging session can improve the design and conduct of clinical trials through both a reduction of sample size and provision of important mechanistic data that may augment clinical trial findings. Moreover, prospective imaging-guided strategies using CMR can enhance safety, efficacy, and cost-effectiveness of cardiovascular pathways in clinical practice around the world. As the future of large-scale clinical trial design evolves to integrate personalized medicine, cost-effectiveness, and mechanistic insights of novel therapies, the integration of CMR will continue to play a critical role. In this document, the attributes, limitations, and challenges of CMR's integration into the future design and conduct of clinical trials will also be covered, and recommendations for trialists will be explored. Several prominent examples of clinical trials that test the efficacy of CMR-imaging guided pathways will also be discussed.},
  langid = {english},
  keywords = {cardiac magnetic resonance,clinical trials},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\ZLPNRPUF\\Rabbat et al_2021_The Future of Cardiac Magnetic Resonance Clinical Trials.pdf}
}

