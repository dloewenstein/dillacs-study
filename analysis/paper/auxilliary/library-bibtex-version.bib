}

@article{miyazaki_dyssynchrony_2010,
  title = {Dyssynchrony {{Indices}} to {{Predict Response}} to {{Cardiac Resynchronization Therapy}}: {{A Comprehensive}}, {{Prospective Single-Center Study}}},
  author = {Miyazaki, Chinami and Redfield, Margaret M. and Powell, Brian D. and Lin, Grace M. and Herges, Regina M. and Hodge, David O. and Olson, Lyle J. and Hayes, David L. and Espinosa, Raul E. and Rea, Robert F. and Bruce, Charles J. and Nelson, Susan M. and Miller, Fletcher A. and Oh, Jae K.},
  year = {2010},
  month = jan,
  journal = {Circ Heart Fail},
  doi = {10.1161/CIRCHEARTFAILURE.108.848085},
  abstract = {Background\textemdash Whether mechanical dyssynchrony indices predict reverse remodeling (RR) or clinical response to cardiac resynchronization therapy (CRT) remains controversial. This prospective study evaluated whether echocardiographic dyssynchrony indices predict RR or clinical response after CRT. Methods and Results\textemdash Of 184 heart failure patients with anticipated CRT who were prospectively enrolled, 131 patients with wide QRS and LVEF \&lt; 35\% had 6-month follow-up after CRT implantation. Fourteen dyssynchrony indices (feasibility) by M-mode (94\%), tissue velocity (96\%), tissue Doppler strain (92\%), 2D speckle strain (65-86\%), 3-D echocardiography (79\%), and timing intervals (98\%) were evaluated. Reverse remodeling (RR: end-systolic volume (ESV) reduction {$\geq$} 15\%) occurred in 55\% and more frequently in non-ischemics (71\%) than ischemics (42\%, p=0.002). Overall, only M-mode, tissue Doppler strain, and total isovolumic time (IVT) had an area under the receiver operating characteristic curve (AUC) greater than the line of no information, but none of these were strongly predictive of RR (AUC 0.63 - 0.71). In non-ischemic cardiomyopathy, no dyssynchrony index predicted RR. In ischemic cardiomyopathy, M-mode (AUC=0.67), tissue Doppler strain (AUC=0.79) and IVT (AUC= 0.76) derived indices predicted RR (p\&lt;0.05 for all) although the incremental value was modest. No indices predicted clinical response assessed by Minnesota Living with Heart Failure Questionnaire, six-minute walk, and peak VO2. Conclusions\textemdash These findings are consistent with the Predictors of Response to CRT (PROSPECT) study and do not support use of these dyssynchrony indices to guide use of CRT.},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\9Y8ESUXR\\Miyazaki et al. - 2010 - Dyssynchrony Indices to Predict Response to Cardia.pdf}
}

