@article{thibaultbernard_cardiacresynchronizationtherapy_2013,
  title = {Cardiac {{Resynchronization Therapy}} in {{Patients With Heart Failure}} and a {{QRS Complex}} {$<$}120 {{Milliseconds}}},
  author = {{Thibault Bernard} and {Harel Fran\c{c}ois} and {Ducharme Anique} and {White Michel} and {Ellenbogen Kenneth A.} and {Frasure-Smith Nancy} and {Roy Denis} and {Philippon Fran\c{c}ois} and {Dorian Paul} and {Talajic Mario} and {Dubuc Marc} and {Guerra Peter G.} and {Macle Laurent} and {Rivard L\'ena} and {Andrade Jason} and {Khairy Paul}},
  year = {2013},
  month = feb,
  journal = {Circulation},
  volume = {127},
  number = {8},
  pages = {873--881},
  doi = {10/f4qqvz},
  abstract = {Background\textemdash Although the benefits of cardiac resynchronization therapy are well established in selected patients with heart failure and a prolonged QRS duration, salutary effects in patients with narrow QRS complexes remain to be demonstrated.Methods and Results\textemdash The Evaluation of Resynchronization Therapy for Heart Failure (LESSER-EARTH) trial is a randomized, double-blind, 12-center study that was designed to compare the effects of active and inactive cardiac resynchronization therapy in patients with severe left ventricular dysfunction and a QRS duration {$<$}120 milliseconds. The trial was interrupted prematurely by the Data Safety and Monitoring Board because of futility and safety concerns after 85 patients were randomized. Changes in exercise duration after 12 months were no different in patients with and without active cardiac resynchronization therapy (-0.7 minutes [95\% confidence interval (CI), -2.9 to 1.5] versus 0.8 minutes [95\% CI, -1.2 to 2.9]; P=0.31]. Similarly, no significant differences were observed in left ventricular end-systolic volumes (-6.4 mL [95\% CI, -18.8 to 5.9] versus 3.1 mL [95\% CI, -9.2 to 15.5]; P=0.28) and ejection fraction (3.3\% [95\% CI, 0.7\textendash 6.0] versus 2.1\% [95\% CI, -0.5 to 4.8]; P=0.52). Moreover, cardiac resynchronization therapy was associated with a significant reduction in the 6-minute walk distance (-11.3 m [95\% CI, -31.7 to 9.7] versus 25.3 m [95\% CI, 6.1\textendash 44.5]; P=0.01), an increase in QRS duration (40.2 milliseconds [95\% CI, 34.2\textendash 46.2] versus 3.4 milliseconds [95\% CI, 0.6\textendash 6.2]; P{$<$}0.0001), and a nonsignificant trend toward an increase in heart failure\textendash related hospitalizations (15 hospitalizations in 5 patients versus 4 hospitalizations in 4 patients).Conclusions\textemdash In patients with a left ventricular ejection fraction {$\leq$}35\%, symptoms of heart failure, and a QRS duration {$<$}120 milliseconds, cardiac resynchronization therapy did not improve clinical outcomes or left ventricular remodeling and was associated with potential harm.Clinical Trial RegistrationURL: http://www.clinicaltrials.gov. Unique identifier: NCT00900549.},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\IA3F7I9Z\\Thibault Bernard et al_2013_Cardiac Resynchronization Therapy in Patients With Heart Failure and a QRS.pdf;C\:\\Users\\loewe\\Documents\\Zotero\\storage\\TM2CKQQT\\CIRCULATIONAHA.112.html}
}

@article{thomas_improvingcardiacresynchronisation_2019,
  title = {Improving {{Cardiac Resynchronisation Therapy}}},
  author = {Thomas, George and {Department of Medicine, Division of Cardiology, Cornell University Medical Center, New York, US} and Kim, Jiwon and {Department of Medicine, Division of Cardiology, Cornell University Medical Center, New York, US} and Lerman, Bruce B and {Department of Medicine, Division of Cardiology, Cornell University Medical Center, New York, US}},
