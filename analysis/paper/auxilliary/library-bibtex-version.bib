@article{teo_regionalejectionfraction_2015,
  title = {Regional Ejection Fraction and Regional Area Strain for Left Ventricular Function Assessment in Male Patients after First-Time Myocardial Infarction},
  author = {Teo, Soo-Kng and Vos, F. J. A. and Tan, Ru-San and Zhong, Liang and Su, Yi},
