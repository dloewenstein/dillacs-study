  title = {Effect of {{Left Bundle Branch Block}} on {{Systolic}} and {{Diastolic Function}} of {{Left Ventricle}} in {{Heart Failure}}},
  author = {{\"O}zdemir, Kurtulu{\c s} and Altunkeser, B{\"u}lent Behl{\"u}l and Korkut, Bayram and Toka{\c c}, Mehmet and G{\"o}k, Hasan},
