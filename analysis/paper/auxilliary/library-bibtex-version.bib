@article{ozdemir_effectisolatedleft_2001,
  title = {Effect of the Isolated Left Bundle Branch Block on Systolic and Diastolic Functions of Left Ventricle},
  author = {{\"O}zdemir, Kurtulu{\c s} and Altunkeser, Billent B. and Dani{\c s}, Gidizar and {\"O}zdemir, Ay{\c s}e and Uluca, Yavuz and Toka{\c c}, Mehmet and Telli, Hasan H. and G{\"o}k, Hasan},
  year = {2001},
  month = nov,
  journal = {Journal of the American Society of Echocardiography},
  series = {Thirteenth {{Annual Scientific Sessions}}},
  volume = {14},
  number = {11},
  pages = {1075--1079},
  issn = {0894-7317},
  doi = {10.1067/mje.2001.115655},
  abstract = {Background: We planned this study to evaluate the effects of left bundle branch block (LBBB) on systolic and diastolic functions of left ventricle (LV) that have not previously been investigated in detail. Material and Methods: Forty-five cases diagnosed as isolated LBBB according to the standard electrocardiographic criteria (group 1, mean age: 60 {$\pm$} 12 years) were taken as the case group and 65 cases with normal conduction system (group II, mean age 58 {$\pm$} 14 years) were taken as the control group. Echocardiography was performed to all patients and coronary angiography was performed to 21 patients in group I and 35 patients in group H. In addition to standard systolic and diastolic function parameters, isovolumetric relaxation time (IRT), isovolumetric contraction time (ICT), and ejection time (ET) were measured by echocardiography, and the myocardial performance index (MPI) [(IRT+ICT)/ ET] was calculated. LV end-diastolic pressure was calculated for the patients undergoing coronary angiography. Results: In group I, LV end-systolic diameter was greater (3.1 0.4 cm vs 2.8 0.4 cm, P {$<$} .001) and ejection fraction was lower (64\% {$\pm$} 6\% vs 68\% 6\%, P {$<$} .001) than those of group II. Rapid filling deceleration time and rate was markedly different in group I (respectively, 133 {$\pm$} 50 ms vs 166 {$\pm$} 24 ms, P {$<$} .001; 608 {$\pm$} 291 cm/s2 vs 383 {$\pm$} 116 cm/s2, P {$<$} .001). In addition, it was found that LBBB caused shortening of LV diastolic period and ET markedly (respectively, 347 {$\pm$} 116 ms vs 394 {$\pm$} 106 m P = .03; 255 {$\pm$} 40 ms vs 294 {$\pm$} 21 ms, P {$<$} .001) and prolongation of IRT and ICT (respectively; 124 {$\pm$} 36 ms vs 91 {$\pm$} 16 ms, 96 {$\pm$} 35 ms vs 38 {$\pm$} 9 ms, P {$<$} .001). The MPI was predominantly higher in group I (0.89 {$\pm$} 0.29 vs 0.40 {$\pm$} 0.06, P {$<$} .001). Invasively determined LV end-diastolic pressure was found higher in group I (14 {$\pm$} 3 mm Hg vs 10 {$\pm$} 3 mm Hg, P {$<$} .001). Conclusion: A marked elevation of the LV MPI and end-diastolic pressure, parallel to changes of conventional echocardiography parameters, in patients with isolated LBBB points out that LBBB causes marked deterioration on LV systolic and diastolic functions.},
  langid = {english},
  file = {C\:\\Users\\loewe\\Documents\\Zotero\\storage\\3VA5TZNE\\Özdemir et al_2001_Effect of the isolated left bundle branch block on systolic and diastolic.pdf;C\:\\Users\\loewe\\Documents\\Zotero\\storage\\FLY99GX3\\S0894731701895535.html}
}

