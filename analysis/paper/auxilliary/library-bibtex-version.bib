@article{zweerink_strainimagingpredict_2018,
  title = {Strain {{Imaging}} to {{Predict Response}} to {{Cardiac Resynchronization Therapy}}: {{A Systematic Comparison}} of {{Strain Parameters Using Multiple Imaging Techniques}}},
  shorttitle = {Strain Imaging to Predict Response to Cardiac Resynchronization Therapy},
  author = {Zweerink, Alwin and van Everdingen, Wouter M. and Nijveldt, Robin and Salden, Odette A. E. and Meine, Mathias and Maass, Alexander H. and Vernooy, Kevin and de Lange, Frederik J. and Vos, Marc A. and Croisille, Pierre and Clarysse, Patrick and Geelhoed, Bastiaan and Rienstra, Michiel and van Gelder, Isabelle C. and van Rossum, Albert C. and Cramer, Maarten J. and Allaart, Cornelis P.},
  year = {2018},
  journal = {ESC Heart Fail.},
  volume = {0},
